{
  "name": "vue-dynamic-provide",
  "version": "1.0.0-beta-1",
  "repository": "https://github.com/LinusBorg/vue-dynamic-provide",
  "author": {
    "name": "Thorsten LÃ¼nborg",
    "url": "ttps://github.com/LinusBorg/"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name VueDynamicProvide lib/index.js",
    "build:analyze": "V_APP_ANALYZE=1 yarn build",
    "build:roll": "VUE_APP_ROLL=1 rimraf dist && rollup -c && uglifyjs dist/vue-dynamic-provide.umd.js -c -m > dist/vue-dynamic-provide.umd.min.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:prod": "VUE_APP_TEST_PROD=1 vue-cli-service test:unit",
    "test:unit:ci": "vue-cli-service test:unit --coverage && cat ./coverage/lcov.info > ./node_modules/.bin/coveralls",
    "test:unit:w": "vue-cli-service test:unit --watch"
  },
  "main": "dist/vue-dynamic-provide.common",
  "browser": "dist/vue-dynamic-provide.umd",
  "files": [
    "/lib"
  ],
  "peerDependencies": {
    "vue": ">=2.5"
  },
  "devDependencies": {
    "@babel/plugin-external-helpers": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "coveralls": "^3.0.2",
    "lint-staged": "^7.2.2",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "4.0.0-beta.8",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "uglifyjs": "^2.4.11",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
